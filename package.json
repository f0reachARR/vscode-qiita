{
  "name": "md2qiita",
  "displayName": "md2qiita",
  "description": "Publish your article to Qiita",
  "version": "0.0.1",
  "publisher": "Neetshin",
  "engines": {
    "vscode": "^1.23.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.sayHello"
  ],
  "main": "./out/extension",
  "contributes": {
    "configuration": [
      {
        "title": "Publish your article to Qiita",
        "properties": {
          "md2qiita.token": {
            "type": "string",
            "description": "Access token for Qiita or your Qiita team"
          }
        }
      }
    ],
    "views": {
      "md2qiita": [
        {
          "id": "md2qiita",
          "name": "MD2Qiita"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "md2qiita",
          "title": "MD2Qiita",
          "icon": "resources/qiita.svg"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.3",
    "qiita-js-2": "0.0.1",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^5.1.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.2",
    "vscode": "^1.1.6"
  }
}
