{
  "name": "md2qiita",
  "displayName": "md2qiita",
  "description": "Publish your article to Qiita via VSCode",
  "version": "0.0.1",
  "publisher": "Neetshin",
  "engines": {
    "vscode": "^1.23.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.sayHello"
  ],
  "main": "./out/extension",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "md2qiita",
          "title": "MD2Qiita",
          "icon": "resources/qiita.svg"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.1",
    "@types/node": "^10.3.1",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "qiita-js": "^0.4.3",
    "tslint-microsoft-contrib": "^5.0.3",
    "tslint-react": "^3.6.0"
  }
}
